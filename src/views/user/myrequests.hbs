<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">File</th>
      <th scope="col">Key Type</th>
      <th scope="col">Date</th>
      <th scope="col">State</th>
      <th scope="col"></th>
    </tr>
  </thead>
  <tbody>
    {{#each requests}}    
    <tr>
      <td>{{this.file}}</td>
      <td>{{this.keyType}}</td>
      <td>{{this.date}}</td>
      <td>{{this.state}}</td>
      <td><a class="btn btn btn-outline-success" href="confirm-request?id={{this.idFile}}&type={{this.keyTypeNum}}" role="button">Confirm</a></td>
    </tr>
    {{/each}}
    
  </tbody>
</table>